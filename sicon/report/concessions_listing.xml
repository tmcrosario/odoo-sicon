<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sicon.concessions_listing_report"
            model="ir.actions.report.xml">
        <field name="name">Concessions Listing</field>
        <field name="model">sicon.concessions_listing.report.wizard</field>
        <field name="report_name">concessions_listing</field>
        <field name="report_type">py3o</field>
        <field name="py3o_filetype">pdf</field>
        <field name="py3o_server_id"
               ref="tmc.local_py3o_server" />
        <field name="py3o_is_local_fusion"
               eval="1" />
        <field name="module">sicon</field>
        <field name="py3o_template_fallback">report/concessions_listing.odt</field>
    </record>

</odoo>

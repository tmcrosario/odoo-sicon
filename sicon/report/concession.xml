<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sicon.concession"
            model="ir.actions.report.xml">
        <field name="name">Concession Details</field>
        <field name="model">sicon.concession</field>
        <field name="report_name">concession</field>
        <field name="report_type">py3o</field>
        <field name="py3o_filetype">pdf</field>
        <field name="py3o_server_id"
               ref="local_py3o_server" />
        <field name="py3o_is_local_fusion"
               eval="1" />
        <field name="module">sicon</field>
        <field name="py3o_template_fallback">report/concession.odt</field>
    </record>

</odoo>
